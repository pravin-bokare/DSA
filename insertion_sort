# Time Complexity : O(n^2)
def insertion_sort(arr):
    for i in range(1, len(arr)):
        j = i-1
        key = arr[i]
        while j>=0 and arr[j]>key:
            arr[j+1]=arr[j]
            j-=1
        arr[j+1]=key
            
        
lst = [75, 85, 90, 95, 100, 80]     
insertion_sort(lst)
print(lst)
    
